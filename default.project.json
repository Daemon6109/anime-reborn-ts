{
	"name": "anime-reborn",
	"emitLegacyScripts": false,
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"tree": {
		"$className": "DataModel",
				"ServerScriptService": {
			"$properties": {
				"LoadStringEnabled": true
			},
			"$className": "ServerScriptService"
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					},
							"@rbxts-js": {
						"$path": "node_modules/@rbxts-js"
					},
					"Project": {
						"$className": "Folder",
						"Common": {
							"$path": "places/common/out"
						},
								"Gameplay": {
									"$path": "places/gameplay/out"
								},
								"Lobby": {
									"$path": "places/lobby/out"
								},
								"AFK": {
									"$path": "places/afk/out"
								}
					}
				}
			}
	},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		}
	}
}
