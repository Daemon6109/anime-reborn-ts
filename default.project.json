{
	"name": "anime-reborn",
	"emitLegacyScripts": false,
	"globIgnorePaths": ["**/build.package.json", "**/tsconfig.json"],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$properties": {
				"LoadStringEnabled": true
			},
			"$className": "ServerScriptService"
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"jest_setup": {
				"$path": "scripts/roblox/jest_setup.luau"
			},
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					},
					"@rbxts-js": {
						"$path": "node_modules/@rbxts-js"
					}
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			},
			"$path": "out"
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		}
	}
}
