{
	"name": "roblox-ts-game",
	"emitLegacyScripts": false,
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$properties": {
				"LoadStringEnabled": true
			},
			"$className": "ServerScriptService",
			"TS": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "../../node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "../../node_modules/@flamework"
					},
					"@rbxts-js": {
						"$path": "../../node_modules/@rbxts-js"
					},
					"Project": {
						"$path": "out"
					}
				}
			},
			"TS_Shared": {
				"$path": "out/shared"
			},
			"TS_Cl": {
				"$path": "out/client"
			}
		},
		"Workspace": {
			"$className": "Workspace"
		}
	}
}