/**
 * Helper functions for working with units
 */
export namespace UnitsData {
	/**
	 * Get unit data by name
	 */
	export function getUnit(name: string): UnitData | undefined {
		return UnitsRegistry[name];
	}

	/**
	 * Get all units of a specific rarity
	 */
	export function getUnitsByRarity(rarity: UnitRarity): Record<string, UnitData> {
		const filteredUnits: Record<string, UnitData> = {};
		for (const [name, unit] of pairs(UnitsRegistry)) {
			if (unit.configuration.Rarity === rarity) {
				filteredUnits[name] = unit;
			}
		}
		return filteredUnits;
	}

	/**
	 * Get all units of a specific element
	 */
	export function getUnitsByElement(element: UnitElement): Record<string, UnitData> {
		const filteredUnits: Record<string, UnitData> = {};
		for (const [name, unit] of pairs(UnitsRegistry)) {
			if (unit.configuration.Element === element) {
				filteredUnits[name] = unit;
			}
		}
		return filteredUnits;
	}

	/**
	 * Get all evolution units
	 */
	export function getEvolutionUnits(): Record<string, UnitData> {
		const evolutionUnits: Record<string, UnitData> = {};
		for (const [name, unit] of pairs(UnitsRegistry)) {
			if (unit.evolved !== undefined) {
				evolutionUnits[name] = unit;
			}
		}
		return evolutionUnits;
	}

	/**
	 * Get base units (non-evolutions)
	 */
	export function getBaseUnits(): Record<string, UnitData> {
		const baseUnits: Record<string, UnitData> = {};
		for (const [name, unit] of pairs(UnitsRegistry)) {
			if (unit.evolved === undefined) {
				baseUnits[name] = unit;
			}
		}
		return baseUnits;
	}

	/**
	 * Get units sorted by placement price
	 */
	export function getUnitsByPrice(): Array<[string, UnitData]> {
		const unitsArray: Array<[string, UnitData]> = [];
		for (const [name, unit] of pairs(UnitsRegistry)) {
			unitsArray.push([name, unit]);
		}

		unitsArray.sort((a, b) => {
			return a[1].configuration.PlacementPrice < b[1].configuration.PlacementPrice;
		});

		return unitsArray;
	}

	/**
	 * Get unit display name
	 */
	export function getDisplayName(name: string): string | undefined {
		return UnitsRegistry[name]?.configuration.DisplayName;
	}

	/**
	 * Get unit placement price
	 */
	export function getPlacementPrice(name: string): number | undefined {
		return UnitsRegistry[name]?.configuration.PlacementPrice;
	}

	/**
	 * Get unit rarity
	 */
	export function getRarity(name: string): UnitRarity | undefined {
		return UnitsRegistry[name]?.configuration.Rarity;
	}

	/**
	 * Get unit element
	 */
	export function getElement(name: string): UnitElement | undefined {
		return UnitsRegistry[name]?.configuration.Element;
	}

	/**
	 * Check if unit is evolved
	 */
	export function isEvolved(name: string): boolean {
		return UnitsRegistry[name]?.evolved !== undefined;
	}

	/**
	 * Get base unit for evolution (if applicable)
	 */
	export function getBaseUnit(name: string): string | undefined {
		return UnitsRegistry[name]?.evolved;
	}

	/**
	 * Get unit damage at specific upgrade level
	 */
	export function getDamageAtLevel(name: string, upgradeLevel: number): number | undefined {
		const unit = UnitsRegistry[name];
		if (!unit) return undefined;

		if (upgradeLevel === 0) {
			return unit.configuration.Damage;
		}

		const upgradeInfo = unit.configuration.UpgradesInfo[upgradeLevel];
		return upgradeInfo?.Damage ?? unit.configuration.Damage;
	}

	/**
	 * Get unit attack speed at specific upgrade level
	 */
	export function getAttackSpeedAtLevel(name: string, upgradeLevel: number): number | undefined {
		const unit = UnitsRegistry[name];
		if (!unit) return undefined;

		if (upgradeLevel === 0) {
			return unit.configuration.AttackSpeed;
		}

		const upgradeInfo = unit.configuration.UpgradesInfo[upgradeLevel];
		return upgradeInfo?.AttackSpeed ?? unit.configuration.AttackSpeed;
	}

	/**
	 * Get unit range at specific upgrade level
	 */
	export function getRangeAtLevel(name: string, upgradeLevel: number): number | undefined {
		const unit = UnitsRegistry[name];
		if (!unit) return undefined;

		if (upgradeLevel === 0) {
			return unit.configuration.Range;
		}

		const upgradeInfo = unit.configuration.UpgradesInfo[upgradeLevel];
		return upgradeInfo?.Range ?? unit.configuration.Range;
	}

	/**
	 * Get total upgrade cost to reach specific level
	 */
	export function getTotalUpgradeCost(name: string, targetLevel: number): number | undefined {
		const unit = UnitsRegistry[name];
		if (!unit || targetLevel <= 0 || targetLevel > unit.configuration.MaxUpgrades) {
			return undefined;
		}

		let totalCost = 0;
		for (let level = 1; level <= targetLevel; level++) {
			const upgradeInfo = unit.configuration.UpgradesInfo[level];
			if (!upgradeInfo) return undefined;
			totalCost += upgradeInfo.UpgradePrice;
		}

		return totalCost;
	}

	/**
	 * Check if unit has map affinity
	 */
	export function hasMapAffinity(name: string): boolean {
		return UnitsRegistry[name]?.configuration.MapAffnity !== undefined;
	}

	/**
	 * Get unit's map affinity
	 */
	export function getMapAffinity(name: string): string | undefined {
		return UnitsRegistry[name]?.configuration.MapAffnity;
	}

	/**
	 * Get unit's passive abilities
	 */
	export function getPassives(name: string): readonly string[] | undefined {
		return UnitsRegistry[name]?.configuration.Passives;
	}

	/**
	 * Check if unit is tradable
	 */
	export function isTradable(name: string): boolean {
		return UnitsRegistry[name]?.tradable === true;
	}

	/**
	 * Get unit's max placement amount
	 */
	export function getMaxPlacement(name: string): number | undefined {
		return UnitsRegistry[name]?.configuration.MaxPlacementAmount;
	}

	/**
	 * Check if unit can evolve
	 */
	export function canEvolve(name: string): boolean {
		return UnitsRegistry[name]?.configuration.EvolveData !== undefined;
	}

	/**
	 * Get unit evolution data
	 */
	export function getEvolutionData(name: string): readonly UnitEvolveData[] | undefined {
		return UnitsRegistry[name]?.configuration.EvolveData;
	}

	/**
	 * Get evolution requirements for a unit
	 */
	export function getEvolutionRequirements(name: string): UnitEvolveRequirements | undefined {
		const evolveData = UnitsRegistry[name]?.configuration.EvolveData;
		return evolveData?.[0]?.Requirements;
	}

	/**
	 * Get what unit evolves into
	 */
	export function getEvolvesInto(name: string): string | undefined {
		const evolveData = UnitsRegistry[name]?.configuration.EvolveData;
		return evolveData?.[0]?.EvolvesInto;
	}

	/**
	 * Get evolution display buffs
	 */
	export function getEvolutionBuffs(name: string): readonly string[] | undefined {
		const evolveData = UnitsRegistry[name]?.configuration.EvolveData;
		return evolveData?.[0]?.DisplayBuffs;
	}

	/**
	 * Check if evolution transfers stats
	 */
	export function doesEvolutionTransferStats(name: string): boolean {
		const evolveData = UnitsRegistry[name]?.configuration.EvolveData;
		return evolveData?.[0]?.TransferStats === true;
	}

	/**
	 * TODO: Additional units to be added progressively
	 *
	 * This registry currently contains only 2 sample units out of 271+ total units.
	 * The remaining units need to be extracted from the live game registry and added here.
	 *
	 * Priority units to add next:
	 * - High-tier/popular units (Mythical, Legendary rarity)
	 * - Units with special mechanics (farming units, special abilities)
	 * - Complete evolution chains
	 *
	 * The structure is established and can accommodate all units with their full data.
	 */
}
