{
	"name": "anime-reborn",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"compilenetwork:gameplay": "blink ./places/gameplay/remotes/index.blink",
		"compilenetwork:lobby": "blink ./places/lobby/remotes/index.blink",
		"compilenetwork:common": "blink ./places/common/remotes/index.blink",
		"compilenetwork:afk": "blink ./places/afk/remotes/index.blink",
		"compilenetwork": "npm run compilenetwork:gameplay && npm run compilenetwork:lobby && npm run compilenetwork:common && npm run compilenetwork:afk",
		"build": "rbxtsc -p .",
		"build:gameplay": "rbxtsc -p ./places/gameplay",
		"watch:gameplay": "rbxtsc -w -p ./places/gameplay",
		"serve:gameplay": "rojo serve ./places/gameplay",
		"build:lobby": "rbxtsc -p ./places/lobby",
		"watch:lobby": "rbxtsc -w -p ./places/lobby",
		"serve:lobby": "rojo serve ./places/lobby",
		"build:common": "rbxtsc -p ./places/common",
		"watch:common": "rbxtsc -w -p ./places/common",
		"serve:common": "rojo serve ./places/common",
		"build:afk": "rbxtsc -p ./places/afk",
		"watch:afk": "rbxtsc -w -p ./places/afk",
		"serve:afk": "rojo serve ./places/afk",
		"test": "./scripts/shell/test.sh",
		"test:local": "npm run test:jest",
		"test:local-build": "npm run build && jest",
		"test:shell": "./scripts/shell/test.sh",
		"test:jest": "bash ./scripts/shell/jest-vscode-wrapper.sh",
		"test:coverage": "npm run test:jest -- --coverage",
		"test:debug": "jest --runInBand",
		"docker:build": "docker compose build || docker-compose build",
		"docker:dev": "docker compose up -d roblox-ts-dev || docker-compose up -d roblox-ts-dev",
		"docker:test": "docker compose run --rm roblox-ts-test || docker-compose run --rm roblox-ts-test",
		"docker:build-project": "docker compose run --rm roblox-ts-build || docker-compose run --rm roblox-ts-build",
		"docker:lint": "docker compose run --rm roblox-ts-lint || docker-compose run --rm roblox-ts-lint",
		"docker:shell": "docker compose exec roblox-ts-dev bash || docker-compose exec roblox-ts-dev bash",
		"docker:logs": "docker compose logs -f roblox-ts-dev || docker-compose logs -f roblox-ts-dev",
		"docker:down": "docker compose down || docker-compose down",
		"docker:clean": "(docker compose down -v && docker system prune -f) || (docker-compose down -v && docker system prune -f)",
		"lint": "eslint places/*/src/**/*.ts",
		"lint:fix": "eslint places/*/src/**/*.ts --fix"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@isentinel/eslint-config": "^1.2.5",
		"@jest/globals": "^30.0.4",
		"@rbxts/compiler-types": "^3.0.0-types.0",
		"@rbxts/jest": "^3.13.2-alpha.1",
		"@rbxts/jest-globals": "^3.13.2-alpha.1",
		"@rbxts/types": "^1.0.861",
		"@rbxts/ui-labs": "^2.4.0",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.0.13",
		"eslint": "^9.30.1",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.5.1",
		"eslint-plugin-roblox-ts-x": "^2.0.0",
		"jest": "^30.0.4",
		"jest-mock-extended": "^4.0.0",
		"prettier": "^3.6.2",
		"rbxts-transformer-optimizer": "^1.0.3",
		"roblox-ts": "^3.0.0",
		"ts-jest": "^29.4.0",
		"typescript": "^5.8.3",
		"rbxts-transformer-flamework": "^1.3.1",
		"typescript-eslint": "^8.36.0"
	},
	"dependencies": {
		"@flamework/components": "^1.3.1",
		"@flamework/core": "^1.3.1",
		"@rbxts/character-promise": "^1.0.2",
		"@rbxts/charm": "^0.10.0",
		"@rbxts/charm-sync": "^0.3.0",
		"@rbxts/janitor": "^1.18.3-ts.0",
		"@rbxts/jecs": "^0.8.3",
		"@rbxts/lemon-signal": "^1.2.0",
		"@rbxts/lyra": "^0.6.0",
		"@rbxts/services": "^1.5.5",
		"@rbxts/t": "^3.2.1",
		"@rbxts/validate-tree": "^2.0.2",
		"@rbxts/vide": "^0.5.7"
	}
}
